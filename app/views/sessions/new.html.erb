<div class="container">
  <h3 class="mb-3 mt-3">Login to your Cellr.</h3>
  <%= bootstrap_form_for :user, url: login_path, method: :post do |f| %>
    <%= f.email_field :email %>
    <%= f.password_field :password %>
    <%= f.submit "Log In" %>
  <% end %>
  <p>New user? <%= link_to "Sign up now!", signup_path %></p>
</div>
